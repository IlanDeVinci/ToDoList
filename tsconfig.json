{
	"compilerOptions": {
		"outDir": "lib",
		"target": "ES2022",
		"module": "es2022",
		"noEmitOnError": false,
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
